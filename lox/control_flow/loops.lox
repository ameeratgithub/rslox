var j=1;
while (j<=5){
    print j+" ";
    j=j+1;
}

print "\n---------\n";

for (var i=0; i<5; i=i+1){
    print "Index: "+ i+"\n";
}

print "\n---------\n";


for (var i=0; i<5; i=i+1) {
    for (var j=4;j>i;j=j-1){
        print " ";
    }

    for (var k=i;k>=0;k=k-1){
        print "*";
    }

    print "\n";
}