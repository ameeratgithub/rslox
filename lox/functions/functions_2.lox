fun getGradeFromMarks(marks){
    var grade;
    // else if clause is not currently supported
    if (marks >= 90){
        grade = "A+ Grade";
    }
    else{
        if (marks>=85 and marks<90){
            grade = "A Grade";
        } else{
            grade = "You're not in top 2 grades";
        }
    }
    return grade;
}

var grades = getGradeFromMarks(90);
println(grades);
grades = getGradeFromMarks(85);
println(grades);
grades = getGradeFromMarks(84);
println(grades);